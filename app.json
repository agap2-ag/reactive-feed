{
  "expo": {
    "name": "reactive-feed",
    "slug": "reactive-feed",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Feed of user posts, each associated with an image and a comment section that can be cleared of comments.",
    "githubUrl": "https://github.com/agap2-ag/reactive-feed",
    "android": {
      "package": "com.ago.reactivefeed"
    }
  }
}
